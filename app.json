{
  "expo": {
    "name": "my-app",
    "sdkVersion": "41.0.0",
    "slug": "my-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactNative.app",
      "buildNumber": "1.0.1"
    },
    "android": {
      "versionCode": 1,
      "package": "com.reactNative.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
